<h1 class="display-1 my-2">List page</h1>
<form method="get" id="list">
    <table class="table">
        <tr>
            <th scope="col">_id</th>
            <th scope="col">ubezpieczony</th>
            <th scope="col">benzyna</th>
            <th scope="col">uszkodozny</th>
            <th scope="col">napÄ™d 4x4</th>
        </tr>
        {{#each docs}}
        <tr>
            <td>{{this._id}}</td>
            <td>{{this.ubezpieczony}}</td>
            <td>{{this.benzyna}}</td>
            <td>{{this.uszkodzony}}</td>
            <td>{{this.naped}}</td>
            <td>
                <button formaction="../list&id={{this._id}}" type="submit" class="btn btn-danger">Delete</button>
            </td>
        </tr>
        {{/each}}
    </table>
</form>
<script type="text/javascript">
    function logSubmit(event) {
        confirm("Sure?") ? form.submit() : null
    }

    const form = document.querySelector("#list");
    form.addEventListener('submit', logSubmit)
</script>